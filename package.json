{
  "name": "valyrian.js",
  "version": "0.0.3",
  "description": "Minimal Frontend framework with server side rendering inspired on Mithril and Hyperapp",
  "main": "index.js",
  "repository": "git@github.com:Masquerade-Circus/valyrian.js.git",
  "author": "Masquerade <christian@masquerade-circus.net>",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "dev:server": "nodemon -w ./server -w ./app/dist -w ./dist ./server/index.js",
    "dev:source": "nodemon -w ./lib ./rollup.js",
    "dev:app": "nodemon -w ./app/src ./rollupApp.js",
    "build": "node ./rollup.js build"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "html-element": "^2.2.0",
    "micro": "^9.1.0",
    "micro-ex-router": "^1.5.4",
    "node-fetch": "^1.7.3",
    "nodemon": "^1.14.11",
    "rollup": "^0.57.1",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2"
  },
  "jscsConfig": {
    "requireSemicolons": true,
    "validateIndentation": {
      "value": 4,
      "allExcept": [
        "emptyLines"
      ]
    }
  }
}
